{
    "name": "stencil-template",
    "version": "0.0.0",
    "description": "Stencil project template",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.js",
    "collection": "./dist/collection/collection-manifest.json",
    "types": "./dist/types/components.d.ts",
    "repository": "https://github.com/ElSimpatico/stencil-template.git",
    "author": "Aaron Velasco Lopez <tiquismiquisss@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "stencil build",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx --quiet --fix --format stylish --color",
        "postinstall": "husky install",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable",
        "test": "stencil test --spec",
        "storybook": "yarn build && concurrently \"yarn build --watch\" \"start-storybook -p 6006\"",
        "build-storybook": "build-storybook"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-conventional": "^16.2.1",
        "@stencil/eslint-plugin": "^0.4.0",
        "@storybook/addon-actions": "^6.4.22",
        "@storybook/addon-essentials": "^6.4.22",
        "@storybook/addon-links": "^6.4.22",
        "@storybook/html": "^6.4.22",
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "babel-loader": "^8.2.5",
        "concurrently": "^7.1.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.4.0",
        "pinst": "^3.0.0",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@stencil/core": "^2.15.1"
    }
}
